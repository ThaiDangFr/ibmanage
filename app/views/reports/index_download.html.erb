<p id="notice"><%= notice %></p>

<h1>Positions#index</h1>
<p>Find me in app/views/positions/index.html.erb</p>



<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Portfolio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.name %></td>
        <td><%= report.code %></td>
        <td><%= report.portfolio.name %></td>
	<td><%= link_to 'Download', download_report_path(report) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<p><%= link_to 'Retour' , root_path %></p>
